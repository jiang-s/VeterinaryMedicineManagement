<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="org.hibernate.auction">  
  
    <class name="com.js.vmm.entity.Product" >  
        <id name="pId">  
            <generator class="increment" />  
        </id>  
        <property name="pName"/>  
        <property name="pSpecification"/>  
        <property name="pManufacturer"/>  
        <property name="pDate"/>  
        <property name="pInPrice"/>  
        <property name="pOutPrice"/>
    </class>  
    
</hibernate-mapping>  